{
    "name": "boyo/mobica",
    "description": "Laravel notifications with Mobica Bulgaria",
    "keywords": ["laravel", "notifications", "sms", "mobica"],
    "homepage": "https://github.com/boyanraichev/mobica",
    "license": "MIT",
    "authors": [
        {
            "name": "Boyan Raichev",
            "email": "myself@boyanr.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "laravel/framework": ">=6.0",
        "guzzlehttp/guzzle": ">=6.3",
        "boyo/bulglish": "*"
    },
    "autoload": {
		"psr-4": {
			"Boyo\\Mobica\\": "src/"
    	}
	},
    "extra": {
	    "laravel": {
	        "providers": [
	            "Boyo\\Mobica\\MobicaServiceProvider"
	        ],
	        "aliases": {
		        "MobicaChannel": "Boyo\\Mobica\\MobicaChannel"
	        }
	    }
    }
}